{
    "name": "sepiphy/laravel-console",
    "description": "The sepiphy/laravel-console package",
    "keywords": [
        "sepiphy", "laravel", "laravel-console", "console"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/laraveltools",
    "support": {
        "issues": "https://github.com/sepiphy/laraveltools/issues",
        "source": "https://github.com/sepiphy/laraveltools"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "illuminate/console": "5.8.*",
        "illuminate/support": "5.8.*",
        "illuminate/filesystem": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\Laravel\\Console\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Sepiphy\\Laravel\\Console\\ConsoleServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
